    <select id="selectDisasterReviewRpt01AnnuityPayList" resultMap="DisabledDataForPaymentReview">
        SELECT PRTYPE,
               CRIMEDIUM,  
               EVTYP,
               REHCYM,
               OCACCIDENTMK,      
               EVCODE,     
               CRIINISSUL, 
               CRIINJCL1,  
               CRIINJCL2,  
               CRIINJCL3,  
               CRIINPART1, 
               CRIINPART2, 
               CRIINPART3, 
               CRIINJNME1, 
               CRIINJNME2, 
               CRIINJNME3, 
               CRIINJNME4, 
               CRIINJDP1,  
               CRIINJDP2,  
               CRIINJDP3,  
               CRIINJDP4,  
               CRIINJDP5,  
               CRIINJDP6,  
               CRIINJDP7,  
               CRIINJDP8,  
               CRIINJDP9,  
               CRIINJDP10, 
               HOSID,      
               DOCTORNAME1,
               DOCTORNAME2,
               JUDGEDATE,
               FAMAPPMK, 
               DEDUCTDAY,
               DISQUALMK,
               OCACCHOSID,      
               OCACCDOCTORNAME1,
               OCACCDOCTORNAME2,
               EVAPPTYP                
          FROM VIEW_BAAPPEXPAND
         WHERE APNO = #apNo:VARCHAR#
           AND SEQNO = '0000' 
           AND SUBSTR(APNO,1,1) = 'K'   
           AND INSKD = '9'
    </select>
    
