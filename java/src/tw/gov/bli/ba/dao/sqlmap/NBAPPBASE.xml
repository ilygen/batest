    <select id="getOldAgeReviewRpt01NpPayListBy" resultMap="OldAgeReviewRpt01NpPayListResult">
        SELECT A.APPDATE,
               A.APNO,
               A.ISSUYM,
               A.PAYYM,
               A.EVTEENAME,
               A.EVTDT,
               A.ISSUEAMT,
               A.ACCEPTMARK,
               A.MANCHKFLG, 
               A.CLOSEDT, 
       		   A.CLOSEREASON 
        FROM   NBAPPBASE A  
        <dynamic prepend="WHERE">
            <isNotParameterPresent>
                1 = 0
            </isNotParameterPresent>
            <isParameterPresent>
                <![CDATA[
                    A.EVTIDS = #evtIds:VARCHAR#
                    AND ROWNUM <= 1
                ]]>
            </isParameterPresent>
        </dynamic>
        ORDER BY A.ISSUYM, A.PAYYM
    </select>
