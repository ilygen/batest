     <select id="selectDataBy" resultMap="NBAPPBASEResult">
        SELECT t.APNO,
               t.APPDATE,
               t.EVTIDS,
               t.EVTIDNNO,
               t.EVTEEBIRT,
               t.EVTEENAME,
               t.EVTSEX,
               t.EVTIDENT,
               t.DIEDATE,
               t.BENIDS,
               t.BENIDNNO,
               t.BENEENAME,
               t.BENEEBIRT,
               t.BENSEX,
               t.BENEVTREL,
               t.COMMTEL,
               t.MOBILE,
               t.COMMTYPE,
               t.COMMZIP,
               t.COMMADDR,
               t.PAYTYPE,
               t.PAYBANKID,
               t.BRANCHID,
               t.PAYEEACC,
               t.ACCIDN,
               t.ACCNAME,
               t.ACCREL,
               t.GRDIDNNO,
               t.GRDNAME,
               t.GRDBIRTH,
               t.HANDICAPDT,
               t.DIADABDT
          FROM NBAPPBASE t
         WHERE t.APNO = #apNo:VARCHAR#
           AND t.SEQNO = #seqNo:VARCHAR#
    </select>
    
    <select id="selectNpPayDataBy" resultMap="NpPayDataResult">
        SELECT X.APPDATE,  
               X.APNO,     
               X.ISSUYM,
               X.PAYYM,    
               X.EVTEENAME,
               X.EVTDT,    
               X.ISSUEAMT, 
               X.MANCHKFLG,
               Y.CHKDT,    
               Y.APLPAYDATE
          FROM (SELECT APPDATE,  
                       APNO,
                       ISSUYM,     
                       PAYYM,    
                       EVTEENAME,
                       EVTDT,    
                       ISSUEAMT, 
                       MANCHKFLG               
                  FROM NBAPPBASE
                 WHERE EVTIDS = #evtIds:VARCHAR#
                   AND SEQNO = '0000') X,
               (SELECT APNO,
                       ISSUYM,
                       PAYYM,                                         
                       CHKDT,                                   
                       DECODE(APLPAYFLG,'3',APLPAYDATE,' ') AS APLPAYDATE
                  FROM NBDAPR
                 WHERE BENIDS = #evtIds:VARCHAR#
                   AND SEQNO = '0000') Y
          WHERE X.APNO = Y.APNO
            AND X.ISSUYM = Y.ISSUYM
    </select>
    
