    <select id="selectPaymentQueryDisabledNpDataBy" resultMap="PaymentQueryDisabledNpDataResult">
		SELECT T1.APNO,
		       T1.PAYYMS,
		       T1.LABMERGE,
		       T1.DABLEVEL,
		       T1.DABTYPE,
		       T1.DABPART,
		       T1.MANCHKFLG,
		       ( SELECT VALSENI FROM NBDAPR 
		          WHERE APNO = #apNo:VARCHAR# 
		            AND SEQNO = '0000'
		            AND ISSUYM = T1.ISSUYM            
		            AND VERISEQ = (SELECT MAX(TO_NUMBER(VERISEQ))
		                             FROM NBDAPR 
		                            WHERE APNO = #apNo:VARCHAR# 
		                              AND SEQNO = '0000'
		                              AND ISSUYM = T1.ISSUYM)
		            AND PAYYM = (SELECT MAX(TO_NUMBER(PAYYM))
		                           FROM NBDAPR 
		                          WHERE APNO = #apNo:VARCHAR# 
		                            AND SEQNO = '0000'
		                            AND ISSUYM = T1.ISSUYM)
		       )VALSENI,
		       ( SELECT VALSENID FROM NBDAPR 
		          WHERE APNO = #apNo:VARCHAR# 
		            AND SEQNO = '0000'
		            AND ISSUYM = T1.ISSUYM            
		            AND VERISEQ = (SELECT MAX(TO_NUMBER(VERISEQ))
		                             FROM NBDAPR 
		                            WHERE APNO = #apNo:VARCHAR# 
		                              AND SEQNO = '0000'
		                              AND ISSUYM = T1.ISSUYM)
		            AND PAYYM = (SELECT MAX(TO_NUMBER(PAYYM))
		                           FROM NBDAPR 
		                          WHERE APNO = #apNo:VARCHAR# 
		                            AND SEQNO = '0000'
		                            AND ISSUYM = T1.ISSUYM)
		       )VALSENID,
		       T1.ISSUYM
		  FROM NBAPPBASE T1
		 WHERE APNO = #apNo:VARCHAR# 
           AND SEQNO = '0000'
    </select>
    
