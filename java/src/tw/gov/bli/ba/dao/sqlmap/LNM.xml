    </resultMap>
    
    <!-- DataUpdateRpt 01 Begin -->
    <!-- [ -->
    <select id="selectDataUpdateRpt01DataBy" resultMap="LNMResult">
        SELECT A.BANKCD, 
               A.BRNO, 
               A.SEQNO, 
               A.IDN_APLY, 
               A.BRDTE_APLY, 
               A.NAME_APLY, 
               A.PCODE_APLY, 
               A.DATE_APLY, 
               A.MONEY_APLY, 
               A.TELNO1, 
               A.TELNO2, 
               A.ACPTDTE, 
               A.UPDATEDTE, 
               A.EDMRK, 
               A.BRDTE_LIB, 
               A.NAME_LIB, 
               A.PCODE_EXM, 
               A.OK_EXM, 
               A.DATE_EXM, 
               A.MONEY_EXM, 
               A.ACCOUNT_PAY, 
               A.DATE_PAY, 
               A.MONEY_PAY, 
               A.LOAN_TERM, 
               A.AADEMONEY, 
               A.ADETIMES, 
               A.ARINT, 
               A.NRMONEY, 
               A.OWMONEY, 
               A.OWINT, 
               A.ACHKDTE, 
               A.PDATE, 
               TO_NUMBER(A.EMRK) AS EMRK, 
               A.EDATE, 
               A.MNEQ, 
               A.IDN_LIB, 
               A.CITRM_MRK, 
               A.DUP, 
               A.FILLER_05 
        FROM   MBLNM A
        <dynamic prepend="WHERE">
            <isNotParameterPresent>
                1 = 0
            </isNotParameterPresent>
            <isParameterPresent>
                <isNotEmpty prepend="AND" property="idnAply">
                    (A.IDN_APLY = #idnAply:VARCHAR# or A.IDN_LIB=#idnAply:VARCHAR#) 
                </isNotEmpty>
                <isNotEmpty prepend="AND" property="brDteAply">
                    A.BRDTE_APLY = #brDteAply:VARCHAR#
                </isNotEmpty>
                <![CDATA[
                AND ROWNUM <= 1
                ]]>
            </isParameterPresent>
        </dynamic>
    </select>
    <!-- ] -->
	<!-- DataUpdateRpt 01 End -->
     
    <!-- DataUpdateRpt 02 Begin -->
    <!-- [ -->
    <select id="selectDataUpdateRpt02DataBy" resultMap="LNMResult">
        SELECT A.BANKCD, 
               A.BRNO, 
               A.SEQNO, 
               A.IDN_APLY, 
               A.BRDTE_APLY, 
               A.NAME_APLY, 
               A.PCODE_APLY, 
               A.DATE_APLY, 
               A.MONEY_APLY, 
               A.TELNO1, 
               A.TELNO2, 
               A.ACPTDTE, 
               A.UPDATEDTE, 
               A.EDMRK, 
               A.BRDTE_LIB, 
               A.NAME_LIB, 
               A.PCODE_EXM, 
               A.OK_EXM, 
               A.DATE_EXM, 
               A.MONEY_EXM, 
               A.ACCOUNT_PAY, 
               A.DATE_PAY, 
               A.MONEY_PAY, 
               A.LOAN_TERM, 
               A.AADEMONEY, 
               A.ADETIMES, 
               A.ARINT, 
               A.NRMONEY, 
               A.OWMONEY, 
               A.OWINT, 
               A.ACHKDTE, 
               A.PDATE, 
               TO_NUMBER(A.EMRK) AS EMRK, 
               A.EDATE, 
               A.MNEQ, 
               A.IDN_LIB, 
               A.CITRM_MRK, 
               A.DUP, 
               A.FILLER_05 
        FROM   MBLNM A
        <dynamic prepend="WHERE">
            <isNotParameterPresent>
                1 = 0
            </isNotParameterPresent>
            <isParameterPresent>
                <isNotEmpty prepend="AND" property="idnAply">
                    A.IDN_APLY = #idnAply:VARCHAR#
                </isNotEmpty>
                <isNotEmpty prepend="AND" property="brDteAply">
                    A.BRDTE_APLY = #brDteAply:VARCHAR#
                </isNotEmpty>
                <![CDATA[
                AND (A.DATE_PAY IS NULL OR A.DATE_PAY = '')
                AND (A.DATE_EXM IS NOT NULL OR A.DATE_EXM <> '')
                AND ROWNUM <= 1
                ]]>
            </isParameterPresent>
        </dynamic>
     </select>
     <!-- ] -->
     <!-- DataUpdateRpt 02 End -->
     
</sqlMap>