    <!-- [ -->
    <select id="getOldAgeReviewRpt01DiePayListBy" resultMap="OldAgeReviewRpt01DiePayListResult">
        SELECT A.BMAPNO,
               A.BMAPDTE,
               A.BMEVNAME,
               A.BMCHKAMT,
               A.BMEXMDTE,
               A.BMPAYDTE,
               A.BMNREPDATE,
               A.BMNDOCMK,
               A.BMNOPDATE,
               A.BMEVTDTE,
               '' AS BMPAYYM
        FROM   BEBMSA A  
        <dynamic prepend="WHERE">
            <isNotParameterPresent>
                1 = 0
            </isNotParameterPresent>
            <isParameterPresent>
                <isNotEmpty prepend="AND" property="evtIdnNo">
                    A.BMEVIDNO = #evtIdnNo:VARCHAR#
                </isNotEmpty>
                <!--  20091109 給付處取消出生日期條件
                <isNotEmpty prepend="AND" property="evtBrDate">
                    A.BMEVBRTH = #evtBrDate:VARCHAR#
                </isNotEmpty>
                -->
                AND A.BMPAYKND = '5'
                AND SUBSTR(A.BMAPNO,5,2) = '51'
            </isParameterPresent>
        </dynamic>
    </select>
    
    <!-- ] -->
    <!-- OldAgeReviewRpt 01 End -->

