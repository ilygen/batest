                          FROM BCCMF09
                         WHERE NVL(TRIM(TAL20),' ') <> '0099'
                           AND (PAYTYP||GROUPMK) LIKE (SUBSTR(#apNo:VARCHAR#,1,2)||'%')
                           AND (SUBSTR(TAL20,1,2) <= SUBSTR(#apNo:VARCHAR#,11,2) AND SUBSTR(TAL20,3,2) >= SUBSTR(#apNo:VARCHAR#,11,2))
                           AND (TRIM(STATUS) IS NULL OR STATUS <> 'D')
