                   FROM NBDAPR 
                  WHERE APNO = #apNo:VARCHAR#
                    AND SEQNO = '0000'
                    AND ISSUYM = T1.ISSUYM)
                   AND PAYYM = (SELECT MAX(TO_NUMBER(PAYYM))
                  FROM NBDAPR 
                  WHERE APNO = #apNo:VARCHAR#
                  AND SEQNO = '0000'
                     AND ISSUYM = T1.ISSUYM)
                  )VALSENI,
                  ( SELECT VALSENID FROM NBDAPR 
                   WHERE APNO = #apNo:VARCHAR#
                   AND SEQNO = '0000'
                   AND ISSUYM = T1.ISSUYM            
                   AND VERISEQ = (SELECT MAX(TO_NUMBER(VERISEQ))
                   FROM NBDAPR 
                  WHERE APNO = #apNo:VARCHAR#
                    AND SEQNO = '0000'
                    AND ISSUYM = T1.ISSUYM)
                   AND PAYYM = (SELECT MAX(TO_NUMBER(PAYYM))
                  FROM NBDAPR 
                  WHERE APNO = #apNo:VARCHAR#
                  AND SEQNO = '0000'
